  <div class="summary-payment">

  </div>


  <div class="payment-container">
    <div class="row">
      <form class="col s12" action="api/charges" method="POST" id="payment-form">
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input id="first_name" type="text" class="validate">
            <label for="first_name">First Name</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input id="last_name" type="text" class="validate">
            <label for="last_name">Last Name</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">email</i>
            <input id="email" type="email" class="validate">
            <label for="email">Email</label>
          </div>
        </div>
        <span class="payment-errors"></span>
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">credit_card</i>
            <input id="cardNo" type="number" size="16" data-stripe="number" value="4242424242424242" class="validate">
            <label for="cardNo">Card Number</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <input id="exp_m" type="number" size="2" data-stripe="exp_month" value="10" class="validate">
            <label for="exp_m">Expiration Month (MM)</label>
          </div>
          <div class="input-field col s6">
            <input id="exp_yr" type="text" size="2" data-stripe="exp_year" value="17" class="validate">
            <label for="exp_yr">Expiration Year (YY)</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input id="cvc" type="number" size="4" data-stripe="cvc" value="123" class="validate">
            <label for="cvc">CVC</label>
          </div>
        </div>
        <input id="total" name="totalPrice" class="total-price" type="hidden" value="100">
        <button id="submit-btn" class="btn waves-effect waves-light submit" type="submit" name="action">Submit
          <i class="material-icons right">send</i>
        </button>
      </form>
    </div>
  </div>
